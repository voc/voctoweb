- conference.mime_type_names do |mime_type, mime_type_name| # for HTML head section
  - if MimeType.is_video(mime_type)
    - feed_url = podcast_folder_video_feed_url(acronym: conference.acronym, mime_type: mime_type_name, quality: FeedQuality::HQ)
  - elsif MimeType.is_audio(mime_type)
    - feed_url = podcast_folder_feed_url(acronym: conference.acronym, mime_type: mime_type_name)
  %link{rel: "alternate", type: "application/rss+xml", title: "Podcast feed #{MimeType.humanized(mime_type)} for this folder", href: feed_url}
